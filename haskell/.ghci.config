-- :set prompt "\x03BB > "
-- :set prompt-cont "\x03BB | "
:set prompt "\ESC[34m位 > \ESC[m"
:set prompt-cont "\ESC[34m位 | \ESC[m"

:set editor vim

-- :unset +t
-- :set +t
-- :set +s
-- :set -fprint-explicit-foralls
-- :set -XNoStarIsType
:set -XTypeApplications
:set -XOverloadedStrings
-- :set -XOverloadedLists

:def normalPrompt \x -> return ":set prompt \"\\ESC[34m位 < \\ESC[m\""
:def insertPrompt \x -> return ":set prompt \"\\ESC[34m位 > \\ESC[m\""
